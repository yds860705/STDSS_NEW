<?xml version="1.0" encoding="utf-8"?>
<rMateMapChart>
	<MapChart id="mainMap">
		<series>
			<MapSeries id="mapseries">
				<filters>
					<DropShadowFilter distance="2" angle="45" color="#888888"/>
				</filters>
				<showDataEffect>
					<SeriesInterpolate duration="1000"/>
				</showDataEffect>
				<stroke>
<!--
alpha 투명도 (기본 1)
color 색상
weight 두께 (기본 1)
-->
					<Stroke color="#444444" weight="0.25" alpha="0.25"/>
				</stroke>
			</MapSeries>
		</series>
	</MapChart>
</rMateMapChart>